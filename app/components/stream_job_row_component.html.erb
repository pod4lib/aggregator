<tr class="<%= "status-#{job.active_job_id}" %>">
  <td class="text-center" aria-hidden="true">
    <%= render JobStatusIconComponent.new(status:, classes: "pod-icon #{job.status}") %>
  </td>
  <td><%= resource_label %></td>
  <td><%= job.class_name.titleize %></td>
  <td><%= local_time(job.created_at, format: datetime_display_format()) %></td>
</tr>